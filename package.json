{
  "name": "vstool",
  "version": "1.0.0",
  "author": "Matthew Bystedt <matthew.bystedt@gov.bc.ca>",
  "description": "Vault sync tool for IIT",
  "bin": {
    "vstool": "./bin/run"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "@types/ejs": "^2.7.0",
    "@types/object-hash": "^1.3.0",
    "@types/request": "^2.48.5",
    "ejs": "^3.0.1",
    "inversify": "^5.0.5",
    "keycloak-admin": "^1.14.8",
    "node-vault": "^0.9.18",
    "object-hash": "^2.0.1",
    "reflect-metadata": "^0.1.13",
    "tslib": "^1.14.1",
    "winston": "^3.3.3",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/jest": "^26.0.7",
    "@types/node": "^12.12.14",
    "@types/node-vault": "^0.9.13",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "eslint": "^6.7.2",
    "eslint-config-google": "^0.14.0",
    "globby": "^10.0.2",
    "jest": "^26.1.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.3",
    "ts-node": "^8.5.4",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "license": "UNLICENSED",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "vstool",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "lint": "eslint \"**/*.{js,ts}\" --quiet --fix --config .eslintrc.json",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint . --ext .ts --fix --config .eslintrc.json",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "jest",
    "e2e": "jest --config=jest.e2e.config.js",
    "start": "ts-node src/index.ts",
    "version": "oclif-dev readme && git add README.md"
  },
  "repository": {
    "type": "git",
    "url": "https://apps.nrs.gov.bc.ca/int/stash/scm/appdev/vault-policy.git"
  },
  "types": "lib/index.d.ts"
}
