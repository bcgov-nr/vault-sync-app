{
  "name": "vstool",
  "version": "1.0.0",
  "author": "Matthew Bystedt <matthew.bystedt@gov.bc.ca>",
  "description": "Vault sync tool for IIT",
  "bin": {
    "vstool": "./bin/run"
  },
  "dependencies": {
    "@oclif/command": "^1.8.36",
    "@oclif/config": "^1.18.17",
    "@oclif/core": "^3.25.3",
    "@oclif/plugin-help": "^6.0.18",
    "@types/ejs": "^3.1.5",
    "axios": "^1.6.8",
    "ejs": "^3.1.9",
    "inversify": "^6.0.2",
    "merge-deep": "^3.0.3",
    "node-vault": "^0.10.2",
    "reflect-metadata": "^0.2.1",
    "tslib": "^2.6.2",
    "winston": "^3.12.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.10",
    "@types/jest": "^29.5.12",
    "@types/merge-deep": "^3.0.3",
    "@types/node": "^20.11.26",
    "@types/request": "^2.48.12",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "license": "UNLICENSED",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "vstool",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "build": "rm -rf lib && tsc",
    "lint": "eslint \"**/*.{js,ts}\" --fix --config .eslintrc.json",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint \"**/*.{js,ts}\" --fix --config .eslintrc.json",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "jest",
    "e2e": "jest --config=jest.e2e.config.js",
    "start": "ts-node src/index.ts",
    "version": "oclif-dev readme && git add README.md"
  },
  "repository": {
    "type": "git",
    "url": "https://apps.nrs.gov.bc.ca/int/stash/scm/appdev/vault-policy.git"
  },
  "types": "lib/index.d.ts"
}
