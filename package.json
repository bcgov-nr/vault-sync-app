{
  "name": "vstool",
  "version": "1.0.0",
  "author": "Matthew Bystedt <matthew.bystedt@gov.bc.ca>",
  "description": "Vault sync tool for IIT",
  "bin": {
    "vstool": "./bin/run"
  },
  "dependencies": {
    "@oclif/command": "^1.8.35",
    "@oclif/config": "^1.18.16",
    "@oclif/plugin-help": "^5.2.17",
    "@types/ejs": "^3.1.2",
    "axios": "^1.4.0",
    "ejs": "^3.1.9",
    "inversify": "^6.0.1",
    "merge-deep": "^3.0.3",
    "node-vault": "^0.10.2",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.6.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.10",
    "@types/jest": "^29.5.3",
    "@types/merge-deep": "^3.0.0",
    "@types/node": "^18.17.5",
    "@types/request": "^2.48.8",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.8.1",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "license": "UNLICENSED",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "vstool",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "build": "rm -rf lib && tsc",
    "lint": "eslint \"**/*.{js,ts}\" --fix --config .eslintrc.json",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint \"**/*.{js,ts}\" --fix --config .eslintrc.json",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "jest",
    "e2e": "jest --config=jest.e2e.config.js",
    "start": "ts-node src/index.ts",
    "version": "oclif-dev readme && git add README.md"
  },
  "repository": {
    "type": "git",
    "url": "https://apps.nrs.gov.bc.ca/int/stash/scm/appdev/vault-policy.git"
  },
  "types": "lib/index.d.ts"
}
