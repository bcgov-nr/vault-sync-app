{
  "name": "vstool",
  "version": "1.0.0",
  "author": "Matthew Bystedt <matthew.bystedt@gov.bc.ca>",
  "description": "Vault sync tool for IIT",
  "bin": {
    "vstool": "./bin/run"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "@types/ejs": "^3.0.6",
    "ejs": "^3.1.6",
    "inversify": "^5.1.1",
    "node-vault": "^0.9.21",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.2.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^12.12.14",
    "@types/node-vault": "^0.9.13",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "license": "UNLICENSED",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "vstool",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "lint": "eslint \"**/*.{js,ts}\" --quiet --fix --config .eslintrc.json",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint . --ext .ts --fix --config .eslintrc.json",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "jest",
    "e2e": "jest --config=jest.e2e.config.js",
    "start": "ts-node src/index.ts",
    "version": "oclif-dev readme && git add README.md"
  },
  "repository": {
    "type": "git",
    "url": "https://apps.nrs.gov.bc.ca/int/stash/scm/appdev/vault-policy.git"
  },
  "types": "lib/index.d.ts"
}
